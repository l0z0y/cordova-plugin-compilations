<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-y-utils" version="1.0.2"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>cordovaYUtils</name>
    <js-module name="cordovaYUtils" src="www/cordovaYUtils.js">
        <clobbers target="window.yUtils" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="cordovaYUtils">
                <param name="android-package" value="com.cordova.YUtils.cordovaYUtils" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/cordovaYUtils.java" target-dir="src/com/cordova/YUtils" />
    </platform>
</plugin>